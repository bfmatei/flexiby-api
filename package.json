{
  "name": "@bfmatei/flexiby-api",
  "version": "1.0.0",
  "description": "Flexiby API",
  "author": "Bogdan Matei <bogdan@matei.dev>",
  "license": "MIT",
  "scripts": {
    "start": "npm run start:dev",
    "nest": "nest",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:clean": "npm run prebuild && npm run build",
    "test": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "e2e": "jest --config test/jest.config.js",
    "prettify": "prettier --write \"./src/**/*.{js,ts,json,yaml}\""
  },
  "dependencies": {
    "@nestjs/common": "6.11.5",
    "@nestjs/config": "0.1.0",
    "@nestjs/core": "6.11.5",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/passport": "6.1.1",
    "@nestjs/platform-express": "6.11.5",
    "@nestjs/swagger": "4.2.6",
    "bcrypt": "3.0.8",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.0",
    "compression": "1.7.4",
    "dotenv": "8.2.0",
    "express-rate-limit": "5.0.0",
    "helmet": "3.21.2",
    "jsonwebtoken": "8.5.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pg": "7.18.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.1",
    "rxjs": "6.5.4",
    "swagger-ui-express": "4.1.3",
    "typeorm": "0.2.22"
  },
  "devDependencies": {
    "@bmatei/eslint-config": "1.0.12",
    "@nestjs/cli": "6.14.1",
    "@nestjs/schematics": "6.9.3",
    "@nestjs/testing": "6.11.5",
    "@types/bcrypt": "3.0.0",
    "@types/compression": "1.7.0",
    "@types/express": "4.17.2",
    "@types/express-rate-limit": "3.3.3",
    "@types/helmet": "0.0.45",
    "@types/jest": "25.1.1",
    "@types/node": "13.7.0",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "eslint": "6.8.0",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.5"
  }
}
